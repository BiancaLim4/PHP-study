<body>
    <form action="" method="GET">
        <input type="text" name="login" placeholder="login">
        <input type="password" name="senha" placeholder="senha">
        <input type="submit" value="Enviar">
    </form>
        
<?php
    if($_GET) {
        // echo "<pre>";
        // print_r($_GET);
        $login = $_GET['login'] ?? '';
        $senha = $_GET['senha'] ?? '';

        echo $login . "=>" . $senha;
    }
?>
</body>
